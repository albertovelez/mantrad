{
  "CurrentVersion": "1",
  "DefaultSite": "{{sitename}}",
  "ComponentsLocations": [
    "{{sitename}}/components"
  ],
  "Apps": {
    "{{appname}}": {
      "Port": 8080,
      "ActiveServices": [
        "middleware",
        "post",
        "get"
      ]
    }
  },
  "Injections": {

  },
  "Port": 8080,
  "ComponentsConfig": {
    "core": {
      "croncleanupevent": "* */5 * * * *",
      "cronbackupevent": "0 */5 * * * *",
      "compressresponses": false,
      "baseurl": "http://localhost:8080/"
    },
    "logs": {
      "logToConsole": "true",
      "daystoremoveoldlogs": 1
    },
    "static": {
      "cached": false
    }
  },
  "Entities": {
    "default": {{{provider}}}
  },
 "Components": [],
   "InactiveComponents": [],
   "GlobalTemplateVars": {
     "global-sitename": "{{projectName}}"
   }
}