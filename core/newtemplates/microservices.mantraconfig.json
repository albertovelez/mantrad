{
  "CurrentVersion": "1",
  "ComponentsLocations": [
    "components"
  ],
  "Apps": {
    "{{appname}}": {
      "Port": 8080,
      "ActiveServices": [
        "middleware",
        "post",
        "get"
      ]
    }
  },
  "Injections": {
  },
  "ComponentsConfig": {
    "core": {
      "croncleanupevent": "* */5 * * * *",
      "cronbackupevent": "0 */5 * * * *",
      "baseurl": "http://localhost:8080/"
    },
    "static": {
      "cached": false
    }
  },
  "Entities": {
    "default": {{{provider}}}
  },
  "DefaultComponents": [],
   "GlobalTemplateVars": {
     "global-sitename": "{{projectName}}"
   }
}